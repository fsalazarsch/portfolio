{
    "openapi": "3.0.0",
    "info": {
      "title": "API",
      "version": "1.0.0"
    },
    "paths": {
      "/blog": {
        "get": {
          "summary": "Obtiene la lista de articulso de blog",
          "operationId": "getBlogs",
          "responses": {
            "200": {
              "description": "Lista de blogs obtenida",
              "content": {
                "application/json": {
                  "url": "./api/blog" 
                }
              }
            }
          }
        }
      },
      "/contact": {
        "get": {
          "summary": "Obtiene la información de contacto",
          "operationId": "getContact",
          "responses": {
            "200": {
              "description": "Información de contacto obtenida",
              "content": {
                "application/json": {
                  "url": "https://mi-api.com/api/contact"  
                }
              }
            }
          }
        }
      }
    }
  }
  